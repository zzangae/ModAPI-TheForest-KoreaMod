<Mod ID="SpawnCommand">
  <Compatible>0.39bp2</Compatible>
  <Button Standard="C" ID="Respawn">
    <Name>
      <EN>Respawn</EN>
    </Name>
    <Description>
      <EN>spawn last prop again</EN>
    </Description>
  </Button>
  <Button Standard="LeftShift+C" ID="RespawnInfinite">
    <Name>
      <EN>RespawnInfinite</EN>
    </Name>
    <Description>
      <EN>Hold button to spawn infinite props</EN>
    </Description>
  </Button>
  <Name>
    <EN>SpawnCommand [0.0.19]</EN>
  </Name>
  <Description>
    <EN>Adds a spawn command to the console</EN>
  </Description>
  <Version>0.0.19</Version>
</Mod>